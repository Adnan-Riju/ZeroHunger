@model IEnumerable<ZeroHungerAssignment.DTO.RequestDTO>

@{
    ViewBag.Title = "Index";
}
<a class="btn btn-primary" href="/Restaurant/index">Show List</a>
<a class="btn btn-danger" href="/Restaurant/History">History</a>
<a class="btn btn-success" href="/Restaurant/AssignEmpl">AssignEmployee</a>
<a class="btn btn-warning" href="/Restaurant/logout">Logout</a>
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.R_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.R_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IteamName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExDate)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.R_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.R_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IteamName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExDate)
            </td>


        </tr>
    }

</table>
